    android:layout_height="match_parent"
    tools:layout_editor_absoluteX="0dp"
    tools:layout_editor_absoluteY="0dp"
    tools:context=".ui.register_patient_activity.other_informations.OtherInformationsFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_margin="8dp"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/parent_gurdian_name_text"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="18sp"
            app:endIconMode="custom"
            app:endIconDrawable="@drawable/ic_mic"
            android:textColorHint="#888888"
            android:hint="@string/parent_gurdian_name">

            <com.google.android.material.textfield.TextInputEditText
                android:id="@+id/parent_gurdian_name"
                android:layout_width="match_parent"
                android:inputType="text"
                android:textSize="18sp"
                android:layout_height="wrap_content" />
        </com.google.android.material.textfield.TextInputLayout>

        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/religion_text"
            style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.ExposedDropdownMenu"
            android:layout_width="match_parent"
            android:layout_marginTop="@dimen/margin_normal"
            android:layout_height="wrap_content"
            android:hint="@string/religion"
            android:textSize="18sp"
            android:textColorHint="#888888">

            <AutoCompleteTextView
                android:id="@+id/religion_dropdown"
                android:layout_width="match_parent"
                android:layout_weight="1"
                android:textSize="18sp"
                android:layout_height="match_parent"
                android:inputType="none" />
        </com.google.android.material.textfield.TextInputLayout>

        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/community_text"
            style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.ExposedDropdownMenu"
            android:layout_width="match_parent"
            android:layout_marginTop="@dimen/margin_normal"
            android:layout_height="wrap_content"
            android:hint="@string/community"
            android:textSize="18sp"
            android:textColorHint="#888888">

            <AutoCompleteTextView
                android:id="@+id/community_dropdown"
                android:layout_width="match_parent"
                android:layout_weight="1"
                android:textSize="18sp"
                android:layout_height="match_parent"
                android:inputType="none" />
        </com.google.android.material.textfield.TextInputLayout>

    </LinearLayout>
